{
  "name": "spb-frontend-ru",
  "description": "SPB Frontend official site",
  "version": "1.0.0",
  "dependencies": {
    "@jacobmischka/gatsby-plugin-react-svg": "^1.1.0",
    "classnames": "^2.2.5",
    "gatsby": "^2.0.35",
    "gatsby-plugin-manifest": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.1",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-source-contentful": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.6",
    "gatsby-transformer-json": "^2.1.5",
    "lodash.throttle": "^4.1.1",
    "marked": "^0.3.18",
    "moment": "^2.21.0",
    "react": "^16.6.0",
    "react-disqussion": "^1.1.0",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-sound": "^1.1.0",
    "rss": "^1.2.2",
    "slug": "^0.9.1",
    "soundmanager2": "^2.97.20170602",
    "translit-rus-eng": "^1.0.7",
    "transliteration": "^1.6.2"
  },
  "engines": {
    "node": "^8.9.1"
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^7.0.5",
    "prettier-eslint-cli": "^4.7.1",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "yaspeller-ci": "^1.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "deploy": "rm -rf public && gatsby build && gh-pages -d public -m Updates [ci skip]",
    "deploy:ci": "rm -rf public && gatsby build && node ghpages.js",
    "develop": "gatsby develop",
    "format": "./node_modules/.bin/prettier-eslint --trailing-comma=\"all\" --no-semi --single-quote --no-bracket-spacing --write  \"gatsby-config.js\" \"gatsby-node.js\"  \"./*.js\" \"src/**/*.js\" \"plugins/**/*.js\"",
    "test": "echo test",
    "lint": "eslint ./src  & npm run spellcheck",
    "spellcheck": "yaspeller-ci ./src/**/*.{js,md}",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint",
      "yaspeller-ci"
    ],
    "*.{css}": [
      "stylelint"
    ],
    "*.md": [
      "yaspeller-ci"
    ]
  }
}
